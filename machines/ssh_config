Host *
    UserKnownHostsFile ./known_hosts/known_hosts

Host bastion
    User root
    HostName lpm-bastion.feri.um.si

Host gateway
    User root
    HostName gateway
    ProxyJump bastion

Host spum-platform
    User root
    HostName spum-platform
    ProxyJump bastion

Host spum-mqtt
    User root
    HostName spum-mqtt
    ProxyJump bastion

Host spum-docker-registry
    User root
    HostName spum-docker-registry
    ProxyJump bastion
